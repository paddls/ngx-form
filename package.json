{
  "name": "@paddls/ngx-form",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project ngx-form-app",
    "build:ngx-form": "ng build --configuration production --project ngx-form && cp README.md dist/ngx-form",
    "test:ngx-form": "ng test --project ngx-form --code-coverage",
    "doc:ngx-form": "typedoc ./projects/ngx-form",
    "test:ci:ngx-form": "npm run test:ngx-form -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:ci": "npm run test:ci:ngx-form",
    "lint": "ng lint",
    "publish:ngx-form": "npm run build:ngx-form && cd dist/ngx-form && npm publish --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.3",
    "@angular/common": "^14.1.3",
    "@angular/compiler": "^14.1.3",
    "@angular/core": "^14.1.3",
    "@angular/forms": "^14.1.3",
    "@angular/platform-browser": "^14.1.3",
    "@angular/platform-browser-dynamic": "^14.1.3",
    "@angular/router": "^14.1.3",
    "lodash.set": "^4.3.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "^14.1.3",
    "@angular/compiler-cli": "^14.1.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.17.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash.clone": "^4.5.0",
    "ng-packagr": "^14.1.0",
    "protractor": "~7.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "~8.3.0",
    "typescript": "~4.6.4"
  }
}
