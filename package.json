{
  "name": "@paddls/ngx-form",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project ngx-form-app",
    "build:ngx-form": "ng build --configuration production --project ngx-form && cp README.md dist/ngx-form",
    "test:ngx-form": "ng test --project ngx-form --code-coverage",
    "doc:ngx-form": "typedoc ./projects/ngx-form",
    "test:ci:ngx-form": "npm run test:ngx-form -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:ci": "npm run test:ci:ngx-form",
    "lint": "ng lint",
    "publish:ngx-form": "npm run build:ngx-form && cd dist/ngx-form && npm publish --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.2",
    "@angular/common": "^18.0.2",
    "@angular/compiler": "^18.0.2",
    "@angular/core": "^18.0.2",
    "@angular/forms": "^18.0.2",
    "@angular/platform-browser": "^18.0.2",
    "@angular/platform-browser-dynamic": "^18.0.2",
    "@angular/router": "^18.0.2",
    "rxjs": "~7.5.3",
    "tslib": "^2.4.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/cli": "^18.0.3",
    "@angular/compiler-cli": "^18.0.2",
    "@types/jasmine": "~4.3.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.57.0",
    "jasmine-core": "~5.1.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^18.0.0",
    "protractor": "~7.0.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "~10.9.1",
    "typescript": "~5.4.5"
  }
}
