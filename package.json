{
  "name": "@paddls/ngx-form",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project ngx-form-app",
    "build:ngx-form": "ng build --prod --project ngx-form && cp README.md dist/ngx-form",
    "test:ngx-form": "ng test --project ngx-form --code-coverage",
    "doc:ngx-form": "typedoc ./projects/ngx-form",
    "test:ci:ngx-form": "npm run test:ngx-form -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:ci": "npm run test:ci:ngx-form",
    "lint": "ng lint",
    "publish:ngx-form": "npm run build:ngx-form && cd dist/ngx-form && npm publish --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "lodash.set": "^4.3.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.3.9",
    "@angular/compiler-cli": "~13.3.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "codelyzer": "^6.0.0",
    "eslint": "^8.17.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash.clone": "^4.5.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "~8.3.0",
    "typescript": "~4.6.4"
  }
}
